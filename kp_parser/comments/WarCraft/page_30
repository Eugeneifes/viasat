<div  class="response good" id="div_review_2404207" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
    
    <meta itemprop="headline" content="Первый блин не&nbsp;комом"/>

    <div itemprop="author" itemscope itemtype="http://schema.org/Person">
        
        <div>
            
                <p class="profile_name"><s></s><a href="/user/7145530/" itemprop="name">Durenmat</a></p>
                <ul class="actions">
