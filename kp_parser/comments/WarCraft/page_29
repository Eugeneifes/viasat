<div  class="response" id="div_review_2460630" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
    
    <meta itemprop="headline" content="Культовый флешбэк"/>

    <div itemprop="author" itemscope itemtype="http://schema.org/Person">
        <img src="https://st.kp.yandex.net/images/users/bsm_1614607-40-499859.jpg" width="46" alt="" title="" itemprop="image" />
