<div  class="response good" id="div_review_2499237" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
    
    <meta itemprop="headline" content="Лучшая экранизация видеоигр"/>

    <div itemprop="author" itemscope itemtype="http://schema.org/Person">
        
        <div>
            
                <p class="profile_name"><s></s><a href="/user/7166439/" itemprop="name">Хроники</a></p>
