<div  class="response" id="div_review_2404420" itemprop="reviews" itemscope itemtype="http://schema.org/Review">
    
    <meta itemprop="headline" content="Экранизация непонятно чего"/>

    <div itemprop="author" itemscope itemtype="http://schema.org/Person">
        <img src="https://st.kp.yandex.net/images/users/bsm_/get-kino-vod-users-avatar/28790/1123059-16-259634.jpg" width="46" alt="" title="" itemprop="image" />
