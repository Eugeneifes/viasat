    function showTrailerPopup(forceDoubleSize){

        if($(".playTrailer").length) {
             return false;
        } else {
           var root = $('.film-img-box');
            $.popupTrailer({
                fid: 277328,
                appendTo: root,
                clickTo: root.find('.popupVideo')
            },forceDoubleSize);
            setTimeout(waitTrailerInit, 50);
        }
    }

    function waitTrailerInit(){
        if($(".playTrailer").length) {
            console.log('clicked');
            $('.dark').hide();
            $(".playTrailer").click();
        } else {
            setTimeout(waitTrailerInit, 50);
        }
    }

    $(function(){
        if(top.location.hash == '#trailer') {
            showTrailerPopup();
        }
        if(top.location.hash == '#trailer_x2') {
            showTrailerPopup(true);
        }
    })

var vodLoaded = false;
</script>

